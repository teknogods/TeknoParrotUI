<?xml version="1.0" encoding="utf-8"?>
<GameProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<GamePath></GamePath>
	<TestMenuParameter></TestMenuParameter>
	<TestMenuIsExecutable>false</TestMenuIsExecutable>
	<ExtraParameters></ExtraParameters>
	<TestMenuExtraParameters></TestMenuExtraParameters>
	<EmulationProfile>EXVS2XB</EmulationProfile>
	<GameProfileRevision>4</GameProfileRevision>
	<HasSeparateTestMode>false</HasSeparateTestMode>
	<EmulatorType>TeknoParrot</EmulatorType>
	<RequiresAdmin>true</RequiresAdmin>
	<ExecutableName>vsac25_Release.exe</ExecutableName>
	<Patreon>true</Patreon>
	<Is64Bit>true</Is64Bit>
	<OnlineProfileURL>https://teknoparrot.com/OnlineProfile/EXVS2XB</OnlineProfileURL>
	<ValidMd5></ValidMd5>
	<ConfigValues>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Input API</FieldName>
			<FieldValue>DirectInput</FieldValue>
			<FieldType>Dropdown</FieldType>
			<FieldOptions>
				<string>DirectInput</string>
				<string>XInput</string>
			</FieldOptions>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>Graphics</CategoryName>
			<FieldName>DisplayMode</FieldName>
			<FieldValue>Fullscreen Windowed</FieldValue>
			<FieldType>Dropdown</FieldType>
			<FieldOptions>
				<string>Windowed</string>
				<string>Fullscreen</string>
				<string>Fullscreen Windowed</string>
			</FieldOptions>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>FpsLimitEnable</FieldName>
			<FieldValue>1</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Limit to 60fps in case you have a HFR monitor</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Small Window</FieldName>
			<FieldValue>1</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Restores the games original 1440x810 window size in windowed mode instad of scaling it to the native game resolution of 1080p</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>TerminalMode</FieldName>
			<FieldValue>0</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Game will boot as a Live Monitor cabinet when this is enabled.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>OnlineID</FieldName>
			<FieldValue></FieldValue>
			<FieldType>Text</FieldType>
			<Hint>Your card ID, click on the Online Profile button to obtain it.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>Network</CategoryName>
			<FieldName>Override Network Adapter</FieldName>
			<FieldValue>0</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Let TP try and emulate a &quot;perfect&quot; network adapter with the specified ip address.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>Network</CategoryName>
			<FieldName>NetworkIp</FieldName>
			<FieldValue>192.168.1.1</FieldValue>
			<FieldType>Text</FieldType>
			<Hint>IP of the network adapter you want to use, ethernet, radmin, etc.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>Network</CategoryName>
			<FieldName>RouterIp</FieldName>
			<FieldValue>192.168.1.1</FieldValue>
			<FieldType>Text</FieldType>
		</FieldInformation>
	</ConfigValues>
	<JoystickButtons>
		<JoystickButtons>
			<ButtonName>Test</ButtonName>
			<InputMapping>Test</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Service</ButtonName>
			<InputMapping>Service1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Coin</ButtonName>
			<InputMapping>Coin1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Start</ButtonName>
			<InputMapping>P1ButtonStart</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Up</ButtonName>
			<InputMapping>P1ButtonUp</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Down</ButtonName>
			<InputMapping>P1ButtonDown</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Left</ButtonName>
			<InputMapping>P1ButtonLeft</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Right</ButtonName>
			<InputMapping>P1ButtonRight</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Button 1</ButtonName>
			<InputMapping>P1Button1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Button 2</ButtonName>
			<InputMapping>P1Button2</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Button 3</ButtonName>
			<InputMapping>P1Button3</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Button 4</ButtonName>
			<InputMapping>P1Button4</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Insert Card</ButtonName>
			<InputMapping>ExtensionOne1</InputMapping>
		</JoystickButtons>
	</JoystickButtons>
</GameProfile>
