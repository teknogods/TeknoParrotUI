<?xml version="1.0" encoding="utf-8"?>
<GameProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GameName>H2Overdrive</GameName>
  <GameGenre>Racing</GameGenre>
  <GamePath/>
  <TestMenuParameter></TestMenuParameter>
  <TestMenuIsExecutable>false</TestMenuIsExecutable>
  <ExtraParameters/>
  <TestMenuExtraParameters/>
  <IconName>Icons\H2Overdrive.png</IconName>
  <EmulationProfile>RawThrillsFNFH2O</EmulationProfile>
  <GameProfileRevision>15</GameProfileRevision>
  <HasSeparateTestMode>false</HasSeparateTestMode>
  <EmulatorType>OpenParrot</EmulatorType>
  <ExecutableName>sdaemon.exe</ExecutableName>
  <ConfigValues>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Windowed</FieldName>
      <FieldValue>0</FieldValue>
      <FieldType>Bool</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Input API</FieldName>
      <FieldValue>DirectInput</FieldValue>
      <FieldType>Dropdown</FieldType>
      <FieldOptions>
        <string>DirectInput</string>
        <string>XInput</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Enable Outputs</FieldName>
      <FieldValue>0</FieldValue>
      <FieldType>Bool</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Use Keyboard/Button For Axis</FieldName>
      <FieldValue>0</FieldValue>
      <FieldType>Bool</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Keyboard/Button Axis Wheel Sensitivity</FieldName>
      <FieldValue>10</FieldValue>
      <FieldType>Slider</FieldType>
      <FieldMin>1</FieldMin>
      <FieldMax>127</FieldMax>
      <FieldOptions />
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Keyboard/Button Axis Pedal Sensitivity</FieldName>
      <FieldValue>10</FieldValue>
      <FieldType>Slider</FieldType>
      <FieldMin>1</FieldMin>
      <FieldMax>255</FieldMax>
      <FieldOptions />
    </FieldInformation>
  </ConfigValues>
  <JoystickButtons>
    <JoystickButtons>
      <ButtonName>Start</ButtonName>
      <InputMapping>P1ButtonStart</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Boost</ButtonName>
      <InputMapping>P1Button1</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>View</ButtonName>
      <InputMapping>P1Button2</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>TEST</ButtonName>
      <InputMapping>P1Button3</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>SERVICE / Menu Countdown Quickening</ButtonName>
      <InputMapping>P1Button4</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Wheel Axis</ButtonName>
      <InputMapping>Analog0</InputMapping>
      <HideWithKeyboardForAxis>true</HideWithKeyboardForAxis>
      <AnalogType>Wheel</AnalogType>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Wheel Axis Left</ButtonName>
      <InputMapping>Analog0</InputMapping>
      <HideWithXInput>true</HideWithXInput>
      <HideWithoutKeyboardForAxis>true</HideWithoutKeyboardForAxis>
      <AnalogType>Wheel</AnalogType>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Wheel Axis Right</ButtonName>
      <InputMapping>Analog0</InputMapping>
      <HideWithXInput>true</HideWithXInput>
      <HideWithoutKeyboardForAxis>true</HideWithoutKeyboardForAxis>
      <AnalogType>Wheel</AnalogType>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Throttle Up Axis (Gas)</ButtonName>
      <InputMapping>Analog2</InputMapping>
      <AnalogType>Gas</AnalogType>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Throttle Down Axis (Brake)</ButtonName>
      <InputMapping>Analog4</InputMapping>
      <AnalogType>Brake</AnalogType>
    </JoystickButtons>
  </JoystickButtons>
</GameProfile>
