<?xml version="1.0" encoding="utf-8"?>
<GameProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<GamePath></GamePath>
	<TestMenuParameter></TestMenuParameter>
	<TestMenuIsExecutable>false</TestMenuIsExecutable>
	<ExtraParameters></ExtraParameters>
	<TestMenuExtraParameters></TestMenuExtraParameters>
	<EmulationProfile>Tetote</EmulationProfile>
	<GameProfileRevision>5</GameProfileRevision>
	<Is64Bit>true</Is64Bit>
	<HasSeparateTestMode>false</HasSeparateTestMode>
	<EmulatorType>TeknoParrot</EmulatorType>
	<ExecutableName>Vision.exe</ExecutableName>
	<Patreon>true</Patreon>
	<OnlineIdFieldName>OnlineID</OnlineIdFieldName>
	<OnlineIdType>NesysId</OnlineIdType>
	<ConfigValues>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Input API</FieldName>
			<FieldValue>DirectInput</FieldValue>
			<FieldType>Dropdown</FieldType>
			<FieldOptions>
				<string>DirectInput</string>
				<string>XInput</string>
			</FieldOptions>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Windowed</FieldName>
			<FieldValue>1</FieldValue>
			<FieldType>Bool</FieldType>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>OnlineID</FieldName>
			<FieldValue></FieldValue>
			<FieldType>Text</FieldType>
			<Hint>Put in your OnlineID from the website. You can click on the "Online Profile" button in the library view of the Teknoparrot UI to get it.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Disable Touch Emulator</FieldName>
			<FieldValue>0</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Tick this option if you want to use a real touchscreen instead of mouse emulation</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Force Specific Monitor</FieldName>
			<FieldValue>0</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Tick this option if you want to use a specific monitor for the game</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Monitor Index</FieldName>
			<FieldValue>0</FieldValue>
			<FieldType>MonitorSelection</FieldType>
			<UseUnitySorting>true</UseUnitySorting>
			<Hint>Select which monitor the game should appear on. Uses Unity's coordinate-based ordering.</Hint>
		</FieldInformation>
	</ConfigValues>
	<JoystickButtons>
		<JoystickButtons>
			<ButtonName>Test</ButtonName>
			<InputMapping>Test</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Service</ButtonName>
			<InputMapping>Service1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Coin 1</ButtonName>
			<InputMapping>Coin1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Select</ButtonName>
			<InputMapping>P1Button2</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Enter</ButtonName>
			<InputMapping>P1Button1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Swipe Card</ButtonName>
			<InputMapping>P1Button3</InputMapping>
		</JoystickButtons>
	</JoystickButtons>
</GameProfile>
