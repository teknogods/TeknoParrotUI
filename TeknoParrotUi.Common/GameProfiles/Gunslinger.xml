<?xml version="1.0" encoding="utf-8"?>
<GameProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GameName>Gunslinger Stratos 3</GameName>
  <GameGenre>Shooter</GameGenre>
  <GamePath/>
  <TestMenuParameter></TestMenuParameter>
  <TestMenuIsExecutable>false</TestMenuIsExecutable>
  <ExtraParameters/>
  <TestMenuExtraParameters/>
  <IconName>Icons\Gunslinger.png</IconName>
  <EmulationProfile>AliensExtermination</EmulationProfile>
  <GameProfileRevision>2</GameProfileRevision>
  <HasSeparateTestMode>false</HasSeparateTestMode>
  <EmulatorType>TeknoParrot</EmulatorType>
  <ExecutableName>game.exe</ExecutableName>
  <Is64Bit>true</Is64Bit>
  <DevOnly>true</DevOnly>
  <Patreon>true</Patreon>
  <ConfigValues>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Input API</FieldName>
      <FieldValue>DirectInput</FieldValue>
      <FieldType>Dropdown</FieldType>
      <FieldOptions>
        <string>DirectInput</string>
        <string>XInput</string>
        <string>RawInput</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>Windowed</FieldName>
      <FieldValue>0</FieldValue>
      <FieldType>Bool</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>DisableLogs</FieldName>
      <FieldValue>1</FieldValue>
      <FieldType>Bool</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>UseLocaleEmulator</FieldName>
      <FieldValue>1</FieldValue>
      <FieldType>Bool</FieldType>
      <Hint>Fixes japanese texts.</Hint>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>General</CategoryName>
      <FieldName>RemoveNesysMessages</FieldName>
      <FieldValue>0</FieldValue>
      <FieldType>Bool</FieldType>
      <Hint>On-screen Nesys messages not displayed.</Hint>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Controls</CategoryName>
      <FieldName>GUN1StickAxisInputStyle</FieldName>
      <FieldValue>UseDPadForGUN1Stick</FieldValue>
      <FieldType>Dropdown</FieldType>
      <Hint>This axis handles character move in-game. Use D-PAD if you already uses Analog to aim.</Hint>
      <FieldOptions>
        <string>UseDPadForGUN1Stick</string>
        <string>UseAnalogAxisForGUN1Stick</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Controls</CategoryName>
      <FieldName>GUN2StickAxisInputStyle</FieldName>
      <FieldValue>UseDPadForGUN2Stick</FieldValue>
      <FieldType>Dropdown</FieldType>
      <Hint>This axis handles character move in-game. Use D-PAD if you already uses Analog to aim.</Hint>
      <FieldOptions>
        <string>UseDPadForGUN2Stick</string>
        <string>UseAnalogAxisForGUN2Stick</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Controls</CategoryName>
      <FieldName>BothGUNSOnSameInput</FieldName>
      <FieldValue>No</FieldValue>
      <Hint>Allow to control both guns with same axis. Just choose which one.</Hint>
      <FieldType>Dropdown</FieldType>
      <FieldOptions>
        <string>No</string>
        <string>GUN1</string>
        <string>GUN2</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Controls</CategoryName>
      <FieldName>GUN1AimingInputStyle</FieldName>
      <FieldValue>UseMouseDeviceToAim</FieldValue>
      <Hint>Choose aiming device style. Analog won't work with RawInput Input API.</Hint>
      <FieldType>Dropdown</FieldType>
      <FieldOptions>
        <string>UseLightGun1DeviceToAim</string>
        <string>UseMouseDeviceToAim</string>
        <string>UseAnalogAxisToAim</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Controls</CategoryName>
      <FieldName>GUN2AimingInputStyle</FieldName>
      <FieldValue>UseMouseDeviceToAim</FieldValue>
      <FieldType>Dropdown</FieldType>
      <Hint>Choose aiming device style. Analog won't work with RawInput Input API.</Hint>
      <FieldOptions>
        <string>UseLightGun2DeviceToAim</string>
        <string>UseMouseDeviceToAim</string>
        <string>UseAnalogAxisToAim</string>
      </FieldOptions>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Network</CategoryName>
      <FieldName>Dhcp</FieldName>
      <FieldValue>1</FieldValue>
      <FieldType>Bool</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Network</CategoryName>
      <FieldName>Ip</FieldName>
      <FieldValue>192.168.1.2</FieldValue>
      <FieldType>Text</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Network</CategoryName>
      <FieldName>Mask</FieldName>
      <FieldValue>255.255.255.0</FieldValue>
      <FieldType>Text</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Network</CategoryName>
      <FieldName>Gateway</FieldName>
      <FieldValue>192.168.1.1</FieldValue>
      <FieldType>Text</FieldType>
    </FieldInformation>
    <FieldInformation>
      <CategoryName>Network</CategoryName>
      <FieldName>Dns1</FieldName>
      <FieldValue>192.168.1.1</FieldValue>
      <FieldType>Text</FieldType>
    </FieldInformation>
  </ConfigValues>
  <JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN Trigger</ButtonName>
      <InputMapping>P1Button6</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN Stick Press</ButtonName>
      <InputMapping>P1Button1</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN Trigger</ButtonName>
      <InputMapping>P2Button5</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN Jump Button</ButtonName>
      <InputMapping>P1Button4</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN Stick Press</ButtonName>
      <InputMapping>P1Button2</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Simulate SIDE-STYLE Guns Combination</ButtonName>
      <InputMapping>P1Button3</InputMapping>
      <Hint>One of the special moves that enables a powerful shooting style.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Simulate TANDEM-STYLE Guns Combination</ButtonName>
      <InputMapping>P1Button5</InputMapping>
      <Hint>The other special move that enables the other powerful shooting style.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Toggle BOTH GUNS / GUN1 only / GUN2 only</ButtonName>
      <InputMapping>P2Button1</InputMapping>
      <Hint>Allow to toggle between currently firing guns. Useful with BothGUNSOnSameAxis.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Combine Guns Reticles</ButtonName>
      <InputMapping>P2Button4</InputMapping>
      <Hint>Combine both guns on screen. That doubles shooting power! Useful with BothGUNSOnSameAxis.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN STICK Button Up</ButtonName>
      <InputMapping>P1ButtonUp</InputMapping>
      <HideWithoutUseDPadForGUN1Stick>true</HideWithoutUseDPadForGUN1Stick>
      <Hint>D-PAD is used to move.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN STICK Button Down</ButtonName>
      <InputMapping>P1ButtonDown</InputMapping>
      <HideWithoutUseDPadForGUN1Stick>true</HideWithoutUseDPadForGUN1Stick>
      <Hint>D-PAD is used to move.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN STICK Button Left</ButtonName>
      <InputMapping>P1ButtonLeft</InputMapping>
      <HideWithoutUseDPadForGUN1Stick>true</HideWithoutUseDPadForGUN1Stick>
      <Hint>D-PAD is used to move.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN STICK Button Right</ButtonName>
      <InputMapping>P1ButtonRight</InputMapping>
      <HideWithoutUseDPadForGUN1Stick>true</HideWithoutUseDPadForGUN1Stick>
      <Hint>D-PAD is used to move.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN STICK X Axis</ButtonName>
      <InputMapping>Analog0</InputMapping>
      <AnalogType>AnalogJoystickReverse</AnalogType>
      <HideWithUseDPadForGUN1Stick>true</HideWithUseDPadForGUN1Stick>
      <HideWithRawInput>true</HideWithRawInput>
      <Hint>Axis is used to move.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN STICK Y Axis</ButtonName>
      <InputMapping>Analog2</InputMapping>
      <AnalogType>AnalogJoystickReverse</AnalogType>
      <HideWithUseDPadForGUN1Stick>true</HideWithUseDPadForGUN1Stick>
      <HideWithRawInput>true</HideWithRawInput>
      <Hint>Axis is used to move.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN AIMING X Axis</ButtonName>
      <InputMapping>Analog0</InputMapping>
      <AnalogType>AnalogJoystick</AnalogType>
      <HideWithoutUseAnalogAxisToAimGUN1>true</HideWithoutUseAnalogAxisToAimGUN1>
      <HideWithRawInput>true</HideWithRawInput>
      <Hint>Axis is used to aim.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN AIMING Y Axis</ButtonName>
      <InputMapping>Analog2</InputMapping>
      <AnalogType>AnalogJoystick</AnalogType>
      <HideWithoutUseAnalogAxisToAimGUN1>true</HideWithoutUseAnalogAxisToAimGUN1>
      <HideWithRawInput>true</HideWithRawInput>
      <Hint>Axis is used to aim.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>LEFT GUN Light Gun</ButtonName>
      <InputMapping>P1LightGun</InputMapping>
      <HideWithDirectInput>true</HideWithDirectInput>
      <HideWithXInput>true</HideWithXInput>
      <Hint>RawInput device#1.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN STICK Button Up</ButtonName>
      <InputMapping>P2ButtonUp</InputMapping>
      <HideWithoutUseDPadForGUN2Stick>true</HideWithoutUseDPadForGUN2Stick>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN STICK Button Down</ButtonName>
      <InputMapping>P2ButtonDown</InputMapping>
      <HideWithoutUseDPadForGUN2Stick>true</HideWithoutUseDPadForGUN2Stick>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN STICK Button Left</ButtonName>
      <InputMapping>P2ButtonLeft</InputMapping>
      <HideWithoutUseDPadForGUN2Stick>true</HideWithoutUseDPadForGUN2Stick>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN STICK Button Right</ButtonName>
      <InputMapping>P2ButtonRight</InputMapping>
      <HideWithoutUseDPadForGUN2Stick>true</HideWithoutUseDPadForGUN2Stick>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN STICK X Axis</ButtonName>
      <InputMapping>Analog4</InputMapping>
      <AnalogType>AnalogJoystickReverse</AnalogType>
      <HideWithUseDPadForGUN2Stick>true</HideWithUseDPadForGUN2Stick>
      <HideWithRawInput>true</HideWithRawInput>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN STICK Y Axis</ButtonName>
      <InputMapping>Analog6</InputMapping>
      <AnalogType>AnalogJoystickReverse</AnalogType>
      <HideWithUseDPadForGUN2Stick>true</HideWithUseDPadForGUN2Stick>
      <HideWithRawInput>true</HideWithRawInput>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN AIMING X Axis</ButtonName>
      <InputMapping>Analog4</InputMapping>
      <AnalogType>AnalogJoystick</AnalogType>
      <HideWithoutUseAnalogAxisToAimGUN2>true</HideWithoutUseAnalogAxisToAimGUN2>
      <HideWithRawInput>true</HideWithRawInput>
      <Hint>Axis is used to aim.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN AIMING Y Axis</ButtonName>
      <InputMapping>Analog6</InputMapping>
      <AnalogType>AnalogJoystick</AnalogType>
      <HideWithoutUseAnalogAxisToAimGUN2>true</HideWithoutUseAnalogAxisToAimGUN2>
      <HideWithRawInput>true</HideWithRawInput>
      <Hint>Axis is used to aim.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>RIGHT GUN Light Gun</ButtonName>
      <InputMapping>P2LightGun</InputMapping>
      <HideWithDirectInput>true</HideWithDirectInput>
      <HideWithXInput>true</HideWithXInput>
      <Hint>RawInput device#2.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>TEST Switch</ButtonName>
      <InputMapping>Test</InputMapping>
      <Hint>Open TEST Menu.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>SELECT Switch</ButtonName>
      <InputMapping>Service1</InputMapping>
      <Hint>Navigate TEST Menu.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>COIN</ButtonName>
      <InputMapping>Coin1</InputMapping>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>SERVICE Switch</ButtonName>
      <InputMapping>Coin2</InputMapping>
      <Hint>Navigate TEST Menu.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>ENTER Switch</ButtonName>
      <InputMapping>P2Button6</InputMapping>
      <Hint>Navigate TEST Menu.</Hint>
    </JoystickButtons>   
    <JoystickButtons>
      <ButtonName>Volume +</ButtonName>
      <InputMapping>P2Button2</InputMapping>
      <Hint>Raise volume.</Hint>
    </JoystickButtons>
    <JoystickButtons>
      <ButtonName>Volume -</ButtonName>
      <InputMapping>P2Button3</InputMapping>
      <Hint>Reduce volume.</Hint>
    </JoystickButtons>
  </JoystickButtons>
</GameProfile>
