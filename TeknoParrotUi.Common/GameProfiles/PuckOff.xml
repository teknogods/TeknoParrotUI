<?xml version="1.0" encoding="utf-8"?>
<GameProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<GamePath></GamePath>
	<TestMenuParameter></TestMenuParameter>
	<TestMenuIsExecutable>false</TestMenuIsExecutable>
	<ExtraParameters></ExtraParameters>
	<TestMenuExtraParameters></TestMenuExtraParameters>
	<ResetHint>false</ResetHint>
	<EmulationProfile>GenericTrackball</EmulationProfile>
	<GameProfileRevision>12</GameProfileRevision>
	<HasSeparateTestMode>false</HasSeparateTestMode>
	<InvertedMouseAxis>true</InvertedMouseAxis>
	<Is64Bit>false</Is64Bit>
	<EmulatorType>ElfLdr2</EmulatorType>
	<Patreon>true</Patreon>
	<DevOnly>false</DevOnly>
	<RequiresAdmin>false</RequiresAdmin>
	<ExecutableName>game</ExecutableName>
	<xAxisMin>0</xAxisMin>
	<xAxisMax>255</xAxisMax>
	<yAxisMin>0</yAxisMin>
	<yAxisMax>255</yAxisMax>
	<ConfigValues>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Input API</FieldName>
			<FieldValue>DirectInput</FieldValue>
			<FieldType>Dropdown</FieldType>
			<FieldOptions>
				<string>DirectInput</string>
				<string>XInput</string>
				<string>RawInputTrackball</string>
			</FieldOptions>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Windowed</FieldName>
			<FieldValue>1</FieldValue>
			<FieldType>Bool</FieldType>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>MSAA Level</FieldName>
			<FieldValue>0x</FieldValue>
			<FieldType>Dropdown</FieldType>
			<FieldOptions>
				<string>0x</string>
				<string>2x</string>
				<string>4x</string>
				<string>8x</string>
			</FieldOptions>
			<Hint>Enables MSAA (Multi Sample Anti Aliasing) at the specified level, if possible on your GPU. </Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Framelimiter</FieldName>
			<FieldValue>1</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Limits the framerate to 60fps. Anything higher causes gameplay issues.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Disable Idle Timer</FieldName>
			<FieldValue>1</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Disables the 60 second inactivity timer if no button or trackball movement has been detected.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>Controls</CategoryName>
			<FieldName>Trackball Sensitivity X</FieldName>
			<FieldValue>100</FieldValue>
			<FieldType>Slider</FieldType>
			<FieldMin>1</FieldMin>
			<FieldMax>200</FieldMax>
			<FieldOptions></FieldOptions>
			<Hint>Scales the trackballs X input. Percent, so 100 = 100%, 25 = 25%</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>Controls</CategoryName>
			<FieldName>Trackball Sensitivity Y</FieldName>
			<FieldValue>100</FieldValue>
			<FieldType>Slider</FieldType>
			<FieldMin>1</FieldMin>
			<FieldMax>200</FieldMax>
			<FieldOptions></FieldOptions>
			<Hint>Scales the trackballs Y input. Percent, so 100 = 100%, 25 = 25%</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Custom Resolution</FieldName>
			<FieldValue>0</FieldValue>
			<FieldType>Bool</FieldType>
			<Hint>Allows setting a custom resolution for the game. Might have minor visual glitches and or  wrong aspect ratio.</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Custom Resolution Width</FieldName>
			<FieldValue>640</FieldValue>
			<FieldType>Text</FieldType>
			<Hint>The games default width is 640</Hint>
		</FieldInformation>
		<FieldInformation>
			<CategoryName>General</CategoryName>
			<FieldName>Custom Resolution Height</FieldName>
			<FieldValue>480</FieldValue>
			<FieldType>Text</FieldType>
			<Hint>The games default height is 480</Hint>
		</FieldInformation>
	</ConfigValues>
	<JoystickButtons>
		<JoystickButtons>
			<ButtonName>Test</ButtonName>
			<InputMapping>Test</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Service</ButtonName>
			<InputMapping>Service1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Coin</ButtonName>
			<InputMapping>Coin1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Coin 2</ButtonName>
			<InputMapping>Coin2</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Door</ButtonName>
			<InputMapping>P1ButtonStart</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Camera</ButtonName>
			<InputMapping>P1Button1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Music</ButtonName>
			<InputMapping>P1Button2</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Comm</ButtonName>
			<InputMapping>P1Button3</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Menu</ButtonName>
			<InputMapping>P2ButtonStart</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Select</ButtonName>
			<InputMapping>P2Button1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>TrackBall</ButtonName>
			<InputMapping>P1Trackball</InputMapping>
			<HideWithDirectInput>true</HideWithDirectInput>
			<HideWithXInput>true</HideWithXInput>
		</JoystickButtons>
	</JoystickButtons>
</GameProfile>
