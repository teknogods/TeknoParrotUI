			<ButtonName>Wheel Axis Left</ButtonName>
			<InputMapping>Analog1</InputMapping>
			<HideWithXInput>true</HideWithXInput>
			<HideWithoutKeyboardForAxis>true</HideWithoutKeyboardForAxis>
			<AnalogType>Wheel</AnalogType>
