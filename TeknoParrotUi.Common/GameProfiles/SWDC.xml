		<JoystickButtons>
			<ButtonName>Wheel Axis Right</ButtonName>
			<InputMapping>Analog1</InputMapping>
			<HideWithXInput>true</HideWithXInput>
			<HideWithoutKeyboardForAxis>true</HideWithoutKeyboardForAxis>
			<AnalogType>Wheel</AnalogType>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Gas</ButtonName>
			<InputMapping>Analog3</InputMapping>
			<AnalogType>Gas</AnalogType>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Brake</ButtonName>
			<InputMapping>Analog5</InputMapping>
			<AnalogType>Brake</AnalogType>
		</JoystickButtons>
		<JoystickButtons>
