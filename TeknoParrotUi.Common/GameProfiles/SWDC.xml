			<InputMapping>P1ButtonLeft</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Menu Right Button</ButtonName>
			<InputMapping>P1ButtonRight</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Shift Down</ButtonName>
			<InputMapping>P2Button1</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
			<ButtonName>Shift Up</ButtonName>
			<InputMapping>P2ButtonStart</InputMapping>
		</JoystickButtons>
		<JoystickButtons>
