<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="using:TeknoParrotUi.Views"
             xmlns:cef="using:CefNet.Avalonia"
             mc:Ignorable="d"
             x:Class="TeknoParrotUi.Views.UserLogin"
             IsVisible="{Binding IsVisible, RelativeSource={RelativeSource Mode=Self}, Mode=TwoWay}"
             PropertyChanged="UserLogin_OnPropertyChanged">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Decorator x:Name="browserWrapper" />
    </Grid>
</UserControl>